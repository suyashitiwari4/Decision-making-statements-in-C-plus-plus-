#include<iostream>
using namespace std;
int main(){
    char ch;
    cout<<"enter the character:"<<endl;
    cin>>ch;
    if(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u'||ch=='A'||ch=='E'||ch=='I'||ch=='O'||ch=='U'){
        cout<<"the character is vowel"<<endl;
    }
    else{
        cout<<"the character is consonant"<<endl;
    }
    return 0;
}
//OUTPUT:
enter the character:
b
the character is consonant 


